
@{
    ViewBag.Title = "Index";
}

<h2>Import</h2>
<br />
<p>
    Please click on the "Choose File" button below and select a CSV file to import.
</p>
@using (Html.BeginForm("Import", "Import", FormMethod.Post, new { enctype = "multipart/form-data"}))
{
    <input type="file" name="upload" />
    <br />
    <input type="submit" value="Import" class="btn-primary" />
}

@Html.Partial("_Success")